<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>log</managed-bean-name>
  <managed-bean-class>myLibrairie.Login</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>reg</managed-bean-name>
  <managed-bean-class>myLibrairie.Register</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>men</managed-bean-name>
  <managed-bean-class>myLibrairie.Menu</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cl</managed-bean-name>
  <managed-bean-class>myLibrairie.Client</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>li</managed-bean-name>
  <managed-bean-class>myLibrairie.Livre</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>com</managed-bean-name>
  <managed-bean-class>myLibrairie.Commande</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/librairies/login.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/librairies/register.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <to-view-id>/librairies/menu.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/librairies/register.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/librairies/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/librairies/menu.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/librairies/client.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <to-view-id>/librairies/commande.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <to-view-id>/librairies/livre.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <to-view-id>/librairies/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/librairies/livre.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/librairies/menu.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/librairies/commande.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/librairies/menu.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/librairies/client.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/librairies/menu.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
</faces-config>
