<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>auth</managed-bean-name>
  <managed-bean-class>foot.Login</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>reg</managed-bean-name>
  <managed-bean-class>foot.Register</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mer</managed-bean-name>
  <managed-bean-class>foot.Mercato</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>play</managed-bean-name>
  <managed-bean-class>foot.Player</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>c</managed-bean-name>
  <managed-bean-class>foot.Coach</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/recrutement/login.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/recrutement/mercato.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <to-view-id>/recrutement/register.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/recrutement/mercato.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/recrutement/player.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <to-view-id>/recrutement/coach.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <to-view-id>/recrutement/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/recrutement/coach.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/recrutement/mercato.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/recrutement/player.xhtml</from-view-id>
  <navigation-case>
   <to-view-id>/recrutement/mercato.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
</faces-config>
